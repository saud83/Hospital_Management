@model PharmaCoHealth.ViewModel.CheckingAppointment
@{
    ViewBag.Title = "CheckAppointments";
    Layout = "~/Views/Shared/_CustomLayout.cshtml";
}

<h3 class="alert-danger text-center">@ViewBag.DoctorName</h3>


<table class="table">
    <tr>
        <th>
            <p>Patient Name</p>
        </th>

        <th>
            <p>Appointment Date</p>
        </th>
    </tr>

    @for (int i = 0, j = 0; i < Model.patient.Count; i++, j++)
    {
        <tr>
            <td>@Model.patient.ElementAt(i)</td>
            <td>@Model.schedule.ElementAt(i)</td>
        </tr>
    }

</table>
@Html.ActionLink("Back to Home", "doctorHome")












