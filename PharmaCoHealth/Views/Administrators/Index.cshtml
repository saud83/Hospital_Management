@model IEnumerable<PharmaCoHealth.Models.Administrator>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_CustomLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.AdminFirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminLastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AdminPhoneNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AdminFirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminLastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminPhoneNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AdminId }) |
            @Html.ActionLink("Details", "Details", new { id=item.AdminId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AdminId })
        </td>
    </tr>
}

</table>
